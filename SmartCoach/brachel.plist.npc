		<ID>intro_vrexpress</ID>
		<script>//This is an alternate intro that uses vrExpress and TTS, instead of dialog within the plist.

engine.begin();

engine.context.presentCharacters.add("Brad");
engine.context.presentCharacters.add("Rachel");

engine.send('vrExpress Brad ALL sbm_gs-all_test_bml_7 &lt;?xml version="1.0" encoding="UTF-8"?&gt; &lt;act&gt; &lt;bml&gt; &lt;body posture="ChrBrad@Idle01"/&gt; &lt;/bml&gt; &lt;/act&gt;');
engine.send('vrExpress Rachel ALL sbm_gs-all_test_bml_8 &lt;?xml version="1.0" encoding="UTF-8"?&gt; &lt;act&gt; &lt;bml&gt; &lt;body posture="ChrRachel_ChrBrad@Idle01"/&gt; &lt;/bml&gt; &lt;/act&gt;');

//These vrExpress messages are received by SmartBody and the TTS system
engine.send('vrExpress Brad user 1303332588320-112-1 &lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;&lt;act&gt;&lt;participant id="brad" role="actor" /&gt;&lt;fml&gt;&lt;turn start="take" end="give" /&gt;&lt;affect type="neutral" target="addressee"&gt;&lt;/affect&gt;&lt;/fml&gt;&lt;bml&gt;&lt;speech id="sp3" ref="vrintro1" type="application/ssml+xml"&gt;Hello and welcome to the VHTK  I am Brad&lt;/speech&gt;&lt;/bml&gt;&lt;/act&gt;');

//Telling NPCEditor to wait X seconds before sending the next command.
engine.rest(4);
            
engine.send('vrExpress Rachel user 1303332588320-122-1 &lt;?xml version="1.0" encoding="UTF-8" standalone="no" ?&gt;&lt;act&gt;&lt;participant id="brad" role="actor" /&gt;&lt;fml&gt;&lt;turn start="take" end="give" /&gt;&lt;affect type="neutral" target="addressee"&gt;&lt;/affect&gt;&lt;/fml&gt;&lt;bml&gt;&lt;speech id="sp2" ref="vrintro2" type="application/ssml+xml"&gt;And I am Rachel.  We are virtual humans.&lt;/speech&gt;&lt;/bml&gt;&lt;/act&gt;');

engine.end();
</script>
